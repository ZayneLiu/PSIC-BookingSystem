<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zayne.psic_booking_system.gui.PatientController">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <ChoiceBox fx:id="choiceBoxSearchBy" prefHeight="25.0" prefWidth="100.0" />
                <ChoiceBox fx:id="choiceBoxExpertise" prefWidth="150.0" />
                <TextField fx:id="textFieldKeyword" />
                <Button fx:id="btnSearch" mnemonicParsing="false" text="Button" />
            </children>
        </HBox>
    </children>
   <padding>
      <Insets left="10.0" top="10.0" />
   </padding>
</VBox>
